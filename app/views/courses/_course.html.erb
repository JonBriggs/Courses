<!--start-->
<div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-2-phone course-container" data-dept='<%=course.department_id%>' data-age='<%=course.current_course_offering.age_level%>' data-duration='<%= course.credits == 3 ? "Y" : "T" %>'>
<div style="text-decoration: none;"
   class='course dept-<%=course.department_id%>'
>

  <div id='course_<%=course.id%>' class="course-card"><!-- class='course-tile'-->
    <%= render partial: "catalog/course_tile", locals: {course:course} %>
  </div>

  <div class='course-info'>
    <%= render partial: "catalog/course_card", locals: {course: course} %>
  </div>
  
</div>
</div>

<script>
$(document).ready(function(){
addClassDialog(<%=course.id%>);
})

</script>
<!--end-->
