<h5>app/views/courses/_course.html.erb</h5>
<span class='dept-<%=course.department_id%> age-<%=course.current_course_offering.age_level%> duration-<%= course.credits == 1 ? "T" : course.credits == 0.5 ? "S" : "Y" %>'>
<a href="javascript:void(0);"
   id='course_<%=course.id%>'
   class='course dept-<%=course.department_id%>'
>
  <div class='course-tile'>
    <%= render partial: "course_tile", locals: {course:course} %>
  </div>
  <div class='course-card'>
    <%= render partial: "course_card", locals: {course: course} %>
  </div>
</a>
</span>
