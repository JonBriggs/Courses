<!--start-->
<div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-2-phone course-container" data-dept='<%=department_card.department_card_id%>' data-age='a' data-duration='a' data-card-type='department'>
<div style="text-decoration: none;"
   class='course dept-<%=department_card.department_card_id%>'
>

  <div id='department_<%=department.id%>' class="course-card"><!-- class='course-tile'-->
    <%= render partial: "catalog/department_tile", locals: {department: department, department_card: department_card} %>
  </div>

  <div class='course-info'>
    <%= render partial: "catalog/department_card", locals: {department: department, department_card: department_card} %>
  </div>
  
</div>
</div>

<script>
$(document).ready(function(){
addDepartmentDialog(<%= department.id%>);
})

</script>
<!--end-->
